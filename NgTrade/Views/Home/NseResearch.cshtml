@{
    Layout = null;
}


<!--=== Breadcrumbs ===-->
<div class="breadcrumbs">
    <div class="container">
        <h1 class="pull-left">{{stock.Stock}} - Current Price: {{stock.Close}} Change: <span data-ng-class="{green: stock.Change > 0, red: stock.Change < 0}">{{stock.Change}}</span></h1>
    </div>
</div><!--/breadcrumbs-->
