@{
    ViewBag.Title = "Nigerian Stock Exchange Trading Online NSE Daily Price List - NgTradeOnline News";
}
 <!-- Start: Main content -->
<div class="content">
    <div class="container">
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- HomePageBottom -->
<ins class="adsbygoogle"
     style="display:inline-block;width:970px;height:90px"
     data-ad-client="ca-pub-5059153330695623"
     data-ad-slot="6904031127"></ins>
<script>
    (adsbygoogle = window.adsbygoogle || []).push({});
</script>
            <div class="page-header">
            <h1>NgTradeOnline<sup style="font-size: 15px;">SM</sup> News <small>Trade Nigerian stocks</small></h1>
        </div>
        <div class="row bottom-space">            
          @foreach (var news in Model)
          {
             <div class="span6">
               @MvcHtmlString.Create(news)
               </div>
          }
          </div>
    </div>
</div>
<!-- End: Main content -->
@section scripts
{
  <script src="~/Scripts/news.js"></script>
}
